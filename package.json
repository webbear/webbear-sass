{
  "name": "webbear-sass-2",
  "version": "1.0.0",
  "description": "frontend framework with a small footprint",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "sass": "sass src/scss/wb.scss --style=expanded --no-source-map dist/css/wb.css",
    "sass:min": "sass src/scss/wb.scss --style=compressed --no-source-map dist/css/wb.min.css",
    "sass:dev": "sass src/scss/wb.scss --style=expanded --no-source-map src/css/wb.css"
  },
  "repository": {
    "type": "git",
    "url": "webbear-sass"
  },
  "keywords": [
    "scss",
    "js"
  ],
  "author": "webbear.ch",
  "license": "ISC",
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "dependencies": {
    "sass": "^1.49.7"
  }
}
